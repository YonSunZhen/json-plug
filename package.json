{
	"name": "json-go-ts",
	"displayName": "json-go-ts",
	"description": "json跳转到ts文件",
	"keywords": [
		"json",
		"ts"
	],
	"version": "0.1.2",
	"publisher": "yonsunzhen",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:json"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "Json Go Ts",
			"properties": {
				"jsonGoTs.fileName": {
					"type": "string",
					"default": "index",
					"description": "关键字"
				},
				"jsonGoTs.filePath": {
					"type": "string",
					"default": "./src/index",
					"description": "以json文件为参照 index文件的相对路径"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"tsc": "tsc -watch -p ./",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./dist/test/runTest.js",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@babel/parser": "^7.14.7",
		"@babel/traverse": "^7.14.7",
		"@types/babel__traverse": "^7.14.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"bugs": {
		"url": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts/issues"
	},
	"repository": {
		"type": "github",
		"url": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts"
	},
	"homepage": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts/blob/master/README.md"
}
