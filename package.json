{
	"name": "json-go-ts",
	"displayName": "json-go-ts",
	"description": "json跳转到ts文件",
	"keywords": [
		"json",
		"ts"
	],
	"version": "0.0.1",
	"publisher": "yonsunzhen",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hello-world.helloWorld",
				"title": "Hello World"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "hello-world.helloWorld",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "Json Go Ts",
			"properties": {
				"jsonGoTs.fileName": {
					"type": "string",
					"default": "index",
					"description": "关键字"
				},
				"jsonGoTs.filePath": {
					"type": "string",
					"default": "./src/index",
					"description": "以json文件为参照 index文件的相对路径"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/parser": "^7.14.7",
		"@babel/traverse": "^7.14.7",
		"@types/babel__traverse": "^7.14.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"bugs": {
		"url": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts/issues"
	},
	"repository": {
		"type": "github",
		"url": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts"
	},
	"homepage": "https://github.com/YonSunZhen/vscode-plugin-json-go-ts/blob/master/README.md"
}
